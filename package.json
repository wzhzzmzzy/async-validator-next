{
  "name": "async-validator-next",
  "version": "0.1.2",
  "packageManager": "pnpm@9.3.0",
  "description": "A successor of async-validator",
  "author": "wzhzzmzzy",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "validator",
    "validate",
    "async"
  ],
  "homepage": "https://github.com/wzhzzmzzy/async-validator-next",
  "bugs": {
    "url": "https://github.com/yiminghe/async-validator-next/issues"
