    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "pnpx unbuild"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@vitest/coverage-istanbul": "^1.6.0",
    "async-validator": "^4.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^9.4.0",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  }
}
